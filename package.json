{
  "name": "modern-real-estate",
  "version": "1.0.0",
  "description": "A modern real estate platform for property listings and management",
  "private": true,
  "keywords": [
    "real-estate",
    "property-listings",
    "nextjs",
    "react"
  ],
  "author": "Modern Real Estate Team",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "clean:artifacts": "node scripts/cleanArtifacts.mjs",
    "analyze": "ANALYZE=true npm run build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "frontend:dev": "next dev",
    "backend:dev": "python3 backend/main.py",
    "seed:demo-users": "node scripts/seed_demo_users.js",
    "smoke": "python3 scripts/smoke_test.py",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.16.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.4.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.542.0",
    "multer": "^2.0.2",
    "next": "15.5.2",
    "pdf-lib": "^1.17.1",
    "prisma": "^6.16.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.19",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "@radix-ui/react-portal": "1.1.9",
    "@radix-ui/react-tooltip": "1.2.8",
    "@radix-ui/react-popper": "1.2.8",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-primitive": "2.1.3",
    "@radix-ui/react-compose-refs": "1.1.2",
    "@radix-ui/react-use-callback-ref": "1.1.1",
    "@radix-ui/react-use-controllable-state": "1.2.2",
    "@radix-ui/react-use-layout-effect": "1.1.1",
    "@radix-ui/react-use-size": "1.1.1",
    "@radix-ui/react-presence": "1.1.5"
  }
